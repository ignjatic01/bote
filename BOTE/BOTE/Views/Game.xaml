<Window x:Class="BOTE.Views.Game"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BOTE.Views"
        mc:Ignorable="d"
        Title="Game" Height="450" Width="800">
    <StackPanel>
        <TextBlock FontSize="20" Text="{Binding Source={StaticResource Player1}, Path=Name}" />
        <TextBlock FontSize="20" Text="{Binding Source={StaticResource Player2}, Path=Name}" />
    </StackPanel>
</Window>
